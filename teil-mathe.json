{
  "teil-mathe": [
    {
      "text": "Was ist die Summe von $x^2$ und $x^2$?",
      "options": ["$2x^2$", "$x^4$", "$x^8$", "$2x^4$"]
    },
    {
      "text": "Was ist das Produkt von $x^2$ und $x^2$?",
      "options": ["$x^4$", "$x^2$", "$2x^2$", "$4x$"]
    },
    {
      "text": "Was ist die Ableitung von $\\sin(x)$?",
      "options": ["$\\cos(x)$", "$\\sin(x)$", "$-\\cos(x)$", "$-\\sin(x)$"]
    },
    {
      "text": "Was ist das Integral von $x$?",
      "options": ["$\\frac{1}{2}x^2 + C$", "$x + C$", "$x^2 + C$", "$\\frac{1}{3}x^3 + C$"]
    },
    {
      "text": "Löse die Gleichung $x^2 - 4 = 0$.",
      "options": ["$x = \\pm 2$", "$x = 2$", "$x = -2$", "$x = 0$"]
    },
    {
      "text": "Was ist die Taylorreihe von $e^x$ um $x = 0$?",
      "options": ["$1 + x + \\frac{x^2}{2!} + \\frac{x^3}{3!} + \\ldots$", "$1 + x$", "$1 - x + \\frac{x^2}{2!} - \\frac{x^3}{3!} + \\ldots$", "$e^x$"]
    },
    {
      "text": "Was ist die zweite Ableitung von $x^3$?",
      "options": ["$6x$", "$3x^2$", "$x^3$", "$3x$"]
    },
    {
      "text": "Was ist die Lösung der Differentialgleichung $\\frac{dy}{dx} = y$?",
      "options": ["$y = Ce^x$", "$y = Cx$", "$y = Cx^2$", "$y = \\sin(x)$"]
    },
    {
      "text": "Berechne den Grenzwert $\\lim_{x \\to \\infty} \\frac{1}{x}$.",
      "options": ["$0$", "$1$", "$\\infty$", "$-\\infty$"]
    },
    {
      "text": "Was ist die Wurzel von $x^2$?",
      "options": ["$|x|$", "$x$", "$\\sqrt{x}$", "$x^2$"]
    }
  ]
}
